<h1><%= @category  %>'s Stuffs </h1>
<p><%= link_to "Go Home!", root_path%></p>

  <% @category.products.each do |product| %>
    <p><%= product.name %></p>
    <p><%= image_tag(product.photo_url)%></p>
    <p>In Stock: <%= product.stock %></p>
    <p>Price: $<%= product.price %></p>
    <p>Sold by: <%= product.name %></p>
  <% end %>
