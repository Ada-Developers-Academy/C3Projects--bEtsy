<p><%= link_to "Go Home", root_path%></p>

<h1>All Products</h1>
<% @products.each do |product| %>
<h3><%= product.category.name %></h3>
<p><%= link_to product.name, product_path(product) %></p>
<p><%= image_tag(product.photo_url)%></p>
<p><%= product.price %></p>
<p><%= product.stock %></p>
<p><%= product.user.username %></p>
<p><%= product.reviews %></p>
<% end %>
